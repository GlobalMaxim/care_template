body {
  font-family: "PF Din Display Pro";
  font-size: 18px;
  line-height: 1.3;
  color: #000;
  margin: 0;
  background: #FFF;
}
@media (max-width: 1599px) {
  body {
    font-size: 16px;
  }
}
@media (max-width: 1199px) {
  body {
    padding-top: 68px;
  }
}
.wrapper {
  position: relative;
  overflow: hidden;
}
.container {
  position: relative;
}
@media (min-width: 1400px) and (max-width: 1600px) {
  .container {
    max-width: 1300px;
  }
}
@media (min-width: 1200px) and (max-width: 1400px) {
  .container {
    max-width: 1200px;
  }
}
@media (min-width: 1600px) {
  .container {
    max-width: 1770px;
  }
}
p {
  margin: 0;
}
.title {
  font-size: 36px;
  font-weight: 700;
  text-transform: uppercase;
}
@media (min-width: 768px) and (max-width: 1199px) {
  .title {
    font-size: 29px;
  }
}
@media (max-width: 767px) {
  .title {
    font-size: 25px;
  }
}
.cl-btn,
.title {
  margin: 0;
  position: relative;
  z-index: 2;
}
a {
  color: #000;
  text-decoration: none;
}
a:hover {
  text-decoration: underline;
  color: #000;
}
a,
button {
  transition: all 0.2s ease-in-out;
}
a:active,
a:focus,
button:active,
button:focus {
  outline: none !important;
}
.form ::-webkit-input-placeholder {
  color: #999;
}
.form ::-moz-placeholder {
  color: #999;
}
.form :-moz-placeholder {
  color: #999;
}
.form :-ms-input-placeholder {
  color: #999;
}
.form label.error {
  display: none !important;
}
.form input,
.form select,
.form textarea {
  border-radius: 0;
  border: 1px solid #ccc;
  color: #000;
  width: 100%;
  outline: none;
  background-color: #fff;
  border-radius: 5px;
}
.form input:focus,
.form select:focus,
.form textarea:focus {
  border-color: #3d3d3d;
}
.form select,
.form input[type="number"] {
  height: auto;
  width: auto;
  padding: 5px 10px;
  height: 40px;
}
.form input {
  height: 40px;
  padding: 0 15px;
}
.form select {
  cursor: pointer;
}
.form textarea {
  padding: 10px 15px;
  resize: none;
}
@media (max-width: 767px) {
  .form textarea {
    height: 100px !important;
  }
}
.form.was-validated .checks input:invalid {
  border-color: red !important;
}
.form .checks {
  text-align: left;
  display: -webkit-flex;
  display: -moz-flex;
  display: -ms-flex;
  display: -o-flex;
  display: flex;
  align-items: center;
}
.form .checks input {
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  width: 24px;
  height: 24px;
  margin: 0 15px 0 0;
  border: 1px solid #171717 !important;
  background: none !important;
  position: relative;
  outline: none;
  flex-shrink: 0;
  padding: 0 !important;
  border-radius: 0;
}
.form .checks input.is-invalid {
  border-color: red !important;
}
.form .checks input:checked:after {
  opacity: 1;
}
.form .checks input:after {
  border-bottom: 2px solid #6c3516;
  border-right: 2px solid #6c3516;
  transform: rotate(43deg);
  width: 9px;
  height: 17px;
  left: 7px;
  top: 0px;
  position: absolute;
  content: "";
  opacity: 0;
  transition: 0.3s;
}
.form .checks label {
  position: relative;
  margin: 0;
  cursor: pointer;
}
@media (max-width: 767px) {
  .form .checks label {
    flex: 1;
  }
}
a.cl-btn,
button.cl-btn {
  display: -webkit-inline-flex;
  display: -moz-inline-flex;
  display: -ms-inline-flex;
  display: -o-inline-flex;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  text-decoration: none;
  color: #fff;
  padding: 10px 25px;
  cursor: pointer;
  border: none;
  position: relative;
  z-index: 2;
  background: #a25627;
  border: 2px solid #a25627;
  text-transform: uppercase;
  font-weight: 700;
